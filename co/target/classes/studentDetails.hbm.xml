<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="co.ayu.dao.StudentDetails" table="STUDENT_DETAILS">
		<id name="studentID" type="java.lang.Integer" >
            <column name="StudentID" length="8"/>
            <generator class="assigned" />
        </id>
		<property name="studentName" type="string">
			<column name="StudentName" />
		</property>
		<property name="studentPhone" type="string">
			<column name="PhoneNo" />
		</property>
		<property name="studentBranch" type="string">
			<column name="Branch" />
		</property>
		<property name="studentAdd" type="string">
			<column name="Address" />
		</property>
	</class>
</hibernate-mapping>